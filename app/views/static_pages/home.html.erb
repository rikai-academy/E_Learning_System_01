                        <%= link_to t('static_pages.home.course_number', numberc: 4), "#course4" , { :class=> '',
                          'data-toggle' => 'tab' }%>
                      </li>
                    </ul>

                    <div class="tab-content">
                      <div id="course1" class="tab-pane active">
                        <ul class="nav nav-tabs">
                          <li class="active">
                            <%= link_to t('static_pages.home.lesson_number', numberl: 1), "#menu2" , { :class=> '',
                              'data-toggle' => 'tab' }%>
